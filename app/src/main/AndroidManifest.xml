<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lata.tripplanner">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MEDIA_CONTENT_CONTROL" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyBmFGNYZusrAqDOBwPj6y3CbqpTCKLjm5M" />

        <activity android:name="com.example.lata.tripplanner.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.lata.tripplanner.LoginActivity" />
        <activity android:name="com.example.lata.tripplanner.SignUpActivity" />
        <activity android:name="com.example.lata.tripplanner.ProfileActivity" />
        <activity android:name="com.example.lata.tripplanner.AddTripActivity" />
        <activity android:name="com.example.lata.tripplanner.ChatRoomActivity" />
        <activity android:name="com.example.lata.tripplanner.SearchListActivity" />
        <activity android:name="com.example.lata.tripplanner.TripHomeActivity" />
        <activity android:name="com.example.lata.tripplanner.TripPlacesActivity" />
        <activity android:name="com.example.lata.tripplanner.MapsActivity" />
        <activity android:name="com.example.lata.tripplanner.DiscoverFriendsActivity"></activity>
    </application>

</manifest>